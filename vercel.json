{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/admin-fixtures-sync?token=fv_cron_92jd81s",
      "schedule": "0 5 * * *"
    },
    {
      "path": "/api/admin-fixtures-sync?token=fv_cron_92jd81s",
      "schedule": "0 13 * * *"
    }
  ]
}
